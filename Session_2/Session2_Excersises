#Excersise 1

def fizzbuzz(n):
   
    for i in range(1, n+1):
        if i % 3 == 0 and i % 5 == 0:
            print("FizzBuzz")
        elif i % 3 == 0:
            print("Fizz")
        elif i % 5 == 0:
            print("Buzz")
        else:
            print(i)

fizzbuzz(20)

#Excersise 2

mixed_list = [10, "hello", 3.14, 25, "world", 42, 9.81]

filtered_integers = [item for item in mixed_list if isinstance(item, int)]

print(filtered_integers)







